<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="my_project_info_action" model="ir.actions.act_window">
        <field name="name">View projects</field>
        <field name="res_model">my.project.info</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="my_project_info_view_form" model="ir.ui.view">
        <field name="name">my.project.info.form</field>
        <field name="model">my.project.info</field>
        <field name="arch" type="xml">
            <form>
                <field name="name" string="Name"/>
                <field name="password" string="Password" password="True"/>
            </form>
        </field>
    </record>
</odoo>